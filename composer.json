{
  "name": "fabianmichael/kirby-clicktoplay",
  "description": "Privacy-friendly video embeds",
  "type": "kirby-plugin",
  "license": "MIT",
	"version": "1.0.0",
  "authors": [
      {
          "name": "Fabian Michael",
          "email": "hallo@fabianmichael.de"
      }
  ],
  "require": {
      "getkirby/composer-installer": "^1.2"
  },
  "autoload": {
      "psr-4": {
          "FabianMichael\\ClickToPlay\\": "src/"
      }
  },
  "config": {
      "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  },
  "extra": {
    "installer-name": "clicktoplay"
  }
}
